        a.time,
        a.direction,
        b.report_code as 'projNo',
        c.name,
        c.card_no as 'cardNo',
        e.report_code as 'groupNo',
        a.proj_id as 'projId',
		ci.corp_code as 'corpCode'
    FROM
        up_worker_signlog_info a
    LEFT JOIN up_project_info b on b.id = a.proj_id
    LEFT JOIN up_worker_info c on c.id = a.worker_id
    LEFT JOIN up_worker_in d on d.worker_id = c.id
    LEFT JOIN up_group_info e on e.id = d.group_id
	LEFT JOIN up_company_info ci on ci.id = e.company_id
    where a.id = #{id}
  </select>
